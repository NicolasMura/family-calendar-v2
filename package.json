    "@nestjs/platform-express": "^7.6.17",
    "@nrwl/angular": "12.3.6",
    "bcrypt": "^5.0.1",
    "hammerjs": "^2.0.8",
    "joi": "^17.4.0",
    "jwt-decode": "^3.1.2",
    "mongoose": "^5.12.13",
    "ngx-webstorage": "^8.0.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
