    "@angular/language-service": "^12.0.0",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/testing": "^7.6.17",
    "@nrwl/cli": "12.3.6",
    "@nrwl/cypress": "12.3.6",
    "@nrwl/eslint-plugin-nx": "12.3.6",
    "@nrwl/jest": "12.3.6",
    "@nrwl/linter": "12.3.6",
    "@nrwl/nest": "^12.3.6",
    "@nrwl/node": "^12.3.6",
    "@nrwl/tao": "12.3.6",
    "@nrwl/workspace": "12.3.6",
